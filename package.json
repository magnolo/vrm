{
  "name": "vrm",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:elements": "ng build --prod --project elements --output-hashing none && npm run pack:elements && cp projects/elements/package.json dist/elements",
    "pack:elements": "bash -c 'cat ./dist/elements/{runtime,polyfills,main}-es5.js > dist/elements/elements.js && ls -lah dist/elements/elements.js'",
    "build:vfa-slider": "ng build --prod --project vfa-slider",
    "build:full": "npm run build:vfa-slider && npm run build:elements && npm run pack:elements"
  },
  "private": true,
